<Window x:Class="EL_PACTÓMETRO.Ventana1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EL_PACTÓMETRO"
        mc:Ignorable="d"
        Title="Ventana1" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <DataGrid Name="dataGridProcesos" AutoGenerateColumns="True" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10" />
        <DataGrid Name="dataGridPartidos" AutoGenerateColumns="True" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="10" />
    </Grid>
</Window>
