<Window x:Class="EL_PACTOMETRO.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EL_PACTOMETRO"
        mc:Ignorable="d"
        Closing="MainWindow_Closing"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="399*"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <DockPanel Grid.ColumnSpan="2">
            <Menu IsMainMenu="True" DockPanel.Dock="Top" Name="menu1" VerticalAlignment="Top">
                <MenuItem Header="Archivo">
                    <MenuItem Header="Exportar" Click="ExportarCSV"/>
                    <MenuItem Header="Importar" Click="ImportarCSV"/>
                </MenuItem>
                <MenuItem Header="_Tablas" Click="Mostrar_Tablas"/>
            </Menu>
        </DockPanel>
    </Grid>
</Window>
